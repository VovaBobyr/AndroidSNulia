<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#11c">
            </stroke>
            <solid android:color="#c00"></solid>
            <size
                android:width="150dp"
                android:height="150dp"></size>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="#c1c">
            </stroke>
            <gradient
                android:startColor="#000"
                android:angle="-90"
                android:endColor="#fff">
            </gradient>
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="#000">
                </stroke>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="-45">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="#000">
                </stroke>
            </shape>
        </rotate>
    </item>
    <item>
        <bitmap
            android:src="@drawable/ic_launcher"
            android:gravity="center">
        </bitmap>
    </item>
    <item>
        <bitmap
            android:src="@drawable/ic_launcher"
            android:tileMode="repeat">
        </bitmap>
    </item>
</layer-list>
