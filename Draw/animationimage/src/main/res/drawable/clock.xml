<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#c00">
            </stroke>
            <size
                android:width="200dp"
                android:height="200dp">
            </size>
        </shape>
    </item>
    <item
        android:left="98dp"
        android:right="5dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#0c0">
            </stroke>
        </shape>
    </item>
</layer-list>